{
  "productName": "Vleer",
  "version": "0.0.1",
  "identifier": "app.vleer",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm nuxt dev --host",
    "beforeBuildCommand": "pnpm nuxt generate"
  },
  "app": {
    "windows": [
      {
        "title": "Vleer",
        "width": 800,
        "height": 600,
        "resizable": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "plugins": {}
}